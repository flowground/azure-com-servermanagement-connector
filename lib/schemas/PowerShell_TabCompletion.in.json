    "properties": {
        "subscriptionId": {
            "required": true,
            "type": "string"
        },
        "resourceGroupName": {
            "required": true,
            "type": "string",
            "minLength": 3,
            "pattern": "[a-zA-Z0-9]+"
        },
        "nodeName": {
            "required": true,
            "type": "string",
            "minLength": 1,
            "maxLength": 256,
            "pattern": "^[a-zA-Z0-9][a-zA-Z0-9_.-]*$"
        },
        "session": {
            "required": true,
            "type": "string"
        },
        "pssession": {
            "required": true,
            "type": "string"
        },
        "requestBody": {
            "description": "Collection of parameters for PowerShell tab completion.",
            "properties": {
                "command": {
                    "description": "Command to get tab completion for.",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "api_version": {
            "required": true,
            "type": "string"
        }
    }
